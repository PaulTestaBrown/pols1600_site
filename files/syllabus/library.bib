@article{Dylko2012,
abstract = {The goals of this study are to explore several claims about the democratizing potential of the internet and to extend gatekeeping theory into user-generated content (UGC) domain. A quantitative content analysis of the most popular YouTube political news videos during the 2008 US presidential election was conducted to investigate the degree to which nonelites were able to partake in mainstream public discourse. We found that elites dominated first and second filters (news sourcing and news production) in the flow of online news, while nonelites dominated the third filter (news distribution). These results suggest that an update to the traditional gatekeeping model is needed to reflect the realities of today's user-driven communication environment.},
author = {Dylko, Ivan B. and Beam, Michael A. and Landreville, Kristen D. and Geidner, Nicholas},
doi = {10.1177/1461444811428899},
issn = {1461-4448},
journal = {New Media {\&} Society},
keywords = {2008 US presidential election,YouTube,elites,empowerment,gatekeeping,internet optimists,internet pessimists,nonelites,sociology of news,user-generated content},
month = {aug},
number = {5},
pages = {832--849},
publisher = {SAGE PublicationsSage UK: London, England},
title = {{Filtering 2008 US presidential election news on YouTube by elites and nonelites: An examination of the democratizing potential of the internet}},
url = {http://journals.sagepub.com/doi/10.1177/1461444811428899},
volume = {14},
year = {2012}
}
