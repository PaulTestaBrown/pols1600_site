isbn = {1068-316X},
issn = {1068-316X},
journal = {Psychology, Crime {\&} Law},
number = {2},
pages = {139--153},
title = {{The influence of sample type, presentation format and strength of evidence on juror simulation research}},
url = {http://dx.doi.org/10.1080/10683160500537431},
volume = {13},
year = {2007}
}
