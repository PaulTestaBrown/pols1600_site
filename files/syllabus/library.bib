isbn = {9781584885740},
issn = {00401706},
pages = {182},
pmid = {25246403},
publisher = {CRC Press},
title = {{Multiple comparisons using R}},
url = {http://www.jstor.org/stable/1266041?origin=crossref},
year = {2011}
}
